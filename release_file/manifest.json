{
  "manifest_version": 2,
  "name": "slide method for Backlog",
  "version": "0.2.1",
  "description": "Backlogのwikiページからスライドを作成",
  "browser_action": {
  "default_popup": "test.html"
  },
  "icons": {
    "128": "on.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "https://*.backlog.jp/wiki/*"
  ],
  "background": {
  "scripts": [ "icon_clicked.js" ],
  "persistent": false
},
"content_scripts": [
  {
    "matches": ["https://*.backlog.jp/wiki/*"],
    "js": ["jquery-2.1.4.min.js", "change.js"],
    "run_at" : "document_end"
  }
]
}
